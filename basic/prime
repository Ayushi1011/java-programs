import java.util.*;
class Main 
{
    public static void main(String args[])
    {
        int i,n;
        boolean r=false;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number to be checked");
        n=sc.nextInt();
        for(i=2;i<n;i++)
        {
            if(n%i==0)
            {
                r=true;
            }
        }
         if(r==true)
         System.out.println("It is not prime number");
         else
         System.out.println("It is a prime number");
    }
}
